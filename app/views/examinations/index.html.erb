<p id="notice"><%= notice %></p>

<h1>Examinations</h1>

<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th>Title</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @examinations.each do |examination| %>
      <tr>
        <td><%= examination.branch_id %></td>
        <td><%= examination.title %></td>
        <td><%= examination.year %></td>
        <td><%= link_to 'Show', examination %></td>
        <td><%= link_to 'Edit', edit_examination_path(examination) %></td>
        <td><%= link_to 'Destroy', examination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', admin_page_main_page_index_path %> | <%= link_to 'New Examination', new_examination_path %>
